                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT 
                         dbo.FnsMSP.id_fns, dbo.FnsMSP.nameOrgS, dbo.FnsMSP.inn, dbo.FnsMSP.dateMSP, 
                         CASE categoryMSP WHEN 1 THEN '1(Микропредприятие)' WHEN 2 THEN '2(Малое предприятие)' WHEN 3 THEN '3(Среднее предприятие)' END AS Категория, dbo.OkvedMSP.kodOKV, dbo.OkvedMSP.nameOKV, 
